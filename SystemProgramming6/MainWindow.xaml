<Window x:Class="SystemProgramming6.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SystemProgramming6"
        mc:Ignorable="d"
        Title="Mutex Demo" Height="350" Width="500">
    <Grid Margin="10">
        <StackPanel>
            <Button Content="Запустити потоки" Click="StartThreads_Click" Height="40"/>
            <TextBox Name="OutputTextBox" Height="250" Margin="0,10,0,0" TextWrapping="Wrap" VerticalScrollBarVisibility="Auto" AcceptsReturn="True"/>
        </StackPanel>
    </Grid>
</Window>
